
#include<iostream>
using namespace std;
int main(){
    int n;
    int i;
    int grade;
    int c;
    cin>>n;
    for(i=0;i<n;i++){
        cin>>grade;
        
        if(grade<38){
            cout<<grade<<endl;
            continue;
        }
        int rem = grade%5;
        if(5-rem<3)
            grade += 5 - rem;
            cout<<grade<<endl;
        }
        return 0;
        
        
        
        
        
        //logic like agar mai leta 8%5 then aayega 1 and (1+1)*2==10 which is next multiple of 5. 
}
